{
  "name": "crystalline-element",
  "version": "1.1.1",
  "description": "A subclass of LitElement built with Ruby2JS in mind.",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "repository": "https://github.com/whitefusionhq/crystalline-element",
  "author": "Jared White",
  "license": "MIT",
  "engines": {
    "node": ">= 10.13.0"
  },
  "scripts": {
    "start": "yarn run build -- -w",
    "clean": "del-cli dist",
    "prebuild": "yarn run clean",
